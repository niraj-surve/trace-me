    "axios",
    "winston"
  ],
  "author": {
    "name": "Niraj Surve",
    "email": "niraj.surve07@gmail.com",
    "url": "https://github.com/niraj-surve"
  },
  "homepage": "https://github.com/niraj-surve/trace-me",
  "bugs": {
    "url": "https://github.com/niraj-surve/trace-me/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^5.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/axios-mock-adapter": "^1.9.0",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "typescript": "^5.3.0"
  }
